    "prettier": "prettier -c src",
    "prettier:format": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "validate": "npm run check-types && npm run eslint && npm run prettier && npm test",
