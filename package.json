  "description": "> [Epignosis](https://www.epignosishq.com/) official react design system.",
  "homepage": "https://github.com/epignosis/gnosis",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
