  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "rimraf dist && tsc --emitDeclarationOnly && rollup --config",
    "build:design-system": "build-storybook -c .storybook -o design-system",
    "validate": "npm run check-types && npm run eslint && npm run prettier && jest --changedSince=main",
    "prepublish": "npm run check-types && npm run eslint && npm run prettier && npm run build",
    "release": "np",
    "check-types": "tsc",
    "eslint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "eslint:fix": "npm run lint:eslint -- --fix",
    "prettier": "prettier -c src",
    "prettier:format": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "author": "John Tsevdos <jtsevdos@epignosishq.com>",
  "license": "MIT",
  "keywords": [
    "epignosis",
    "react",
    "design system"
  ],
