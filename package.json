{
  "name": "@epignosis_llc/gnosis",
  "version": "6.5.4",
  "type": "module",
  "description": "Epignosis official react design system.",
  "homepage": "https://github.com/epignosis/gnosis",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/src/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./icons": {
      "import": "./icons/index.js",
      "require": "./icons/index.cjs"
    }
  },
  "files": [
    "dist",
    "icons"
  ],
  "scripts": {
    "start": "storybook dev -p 6006",
    "build": "rimraf dist && tsc --project tsconfig.build.json --emitDeclarationOnly && rollup --config",
    "build:design-system": "storybook build -c .storybook -o design-system",
    "check-types": "tsc --project tsconfig.check.json --noEmit",
    "eslint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier -c src",
    "prettier:format": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "validate": "npm run check-types && npm run eslint && npm run prettier && npm test",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "author": "John Tsevdos <jtsevdos@epignosishq.com>",
  "license": "MIT",
  "keywords": [
    "epignosis",
    "react",
    "design system",
    "component library"
  ],
  "engines": {
    "node": ">=20.19.0 || >=22.12.0",
    "npm": ">=10.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@chromatic-com/storybook": "^4.1.3",
    "@emotion/babel-plugin": "^11.12.0",
    "@emotion/babel-preset-css-prop": "^11.12.0",
    "@faker-js/faker": "7.6.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-links": "^10.0.8",
    "@storybook/react": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/color": "^3.0.6",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-jest": "^27.5.1",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-storybook": "^10.0.8",
    "jest": "^27.5.1",
    "postcss": "^8.4.47",
    "prettier": "^2.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^19.0.5",
    "storybook": "^10.0.8",
    "typescript": "^4.9.3",
    "vite": "^5.4.21",
    "vite-plugin-svgr": "^4.5.0"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@tippyjs/react": "^4.2.6",
    "ahooks": "^3.8.1",
    "canvas": "^2.11.2",
    "classnames": "^2.5.1",
    "color": "^4.2.3",
    "core-js": "^3.38.1",
    "deepmerge": "^4.3.1",
    "framer-motion": "^6.5.1",
    "react-focus-lock": "2.13.5",
    "react-modal": "^3.16.1",
    "react-select": "^5.8.1",
    "react-spinners": "^0.13.6",
    "tslib": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epignosis/gnosis.git"
  },
  "bugs": {
    "url": "https://github.com/epignosis/gnosis/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
